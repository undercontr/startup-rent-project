datasource db {
    provider = "sqlite"
    url = "file:./db.sqlite"
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id Int @id @default(autoincrement())
    name String
    slug String @unique
    password String
}

model Car {
    id Int @id @default(autoincrement())
    name String 
    image String
    description String
}